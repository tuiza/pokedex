{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/screens/*"],
      "@navigation/*": ["src/navigation/*"],
      "@utils/*": ["src/utils/*"],
      "@assets/*": ["../assets/*"],
      "@hooks/*": ["src/hooks/*"],
      "@types/*": ["src/@types/*"],
      "@services/*": ["src/services/*"],
  }
}
}
